{
    "logger": {
        "prefix": "-logger",
        "body": [
            " logger({",
            "    name: \"${1:Variable}\",",
            "    value: ${1:Variable},",
            "    path: '$RELATIVE_FILEPATH',",
            "    line: '$TM_LINE_NUMBER',",
            "    date: '${CURRENT_YEAR}-${CURRENT_MONTH_NAME}-${CURRENT_DATE}',",
            "    time: '${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}',",
            "    comment: `${2:comment}`",
            "})"
        ],
        "description": "Logger full"
    },
	"console-log-advanced": {
		"prefix": "-log",
		"body": [
			"logger({",
			"  name: \"${1:Variable}\",",
			"  value: ${1:Variable},",	
			"})"
		],
		"description": "Console Log Advanced"
	},
	"date ": {
		"prefix": "-date",
		"body": [
			"date: '${CURRENT_YEAR}-${CURRENT_MONTH_NAME}-${CURRENT_DATE}'"
		],
		"description": "Date"
	},
	"time ": {
		"prefix": "-time",
		"body": [
			"time: '${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}'"
		],
		"description": "Time"
	},
	"path ": {
		"prefix": "-path",
		"body": [
			"path: '$RELATIVE_FILEPATH'"
		],
		"description": "Path"
	},
	"line ": {
		"prefix": "-line",
		"body": [
			"line: '$TM_LINE_NUMBER'"
		],
		"description": "Line"
	},
	"comment ": {
		"prefix": "-comment",
		"body": [
			"comment: `${1:comment}`"
		],
		"description": "comment"
	}
}
